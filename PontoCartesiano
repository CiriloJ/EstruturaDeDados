public class PontoCartesiano extends atividade1 {
	int xa, xb, ya, yb;
	
	public PontoCartesiano(int xa, int xb, int ya, int yb){
		this.xa=xa;
		this.xb=xb;
		this.ya=ya;
		this.yb=yb;
	}

	public int getXa() {
		return xa;
	}

	public void setXa(int xa) {
		this.xa = xa;
	}

	public int getXb() {
		return xb;
	}

	public void setXb(int xb) {
		this.xb = xb;
	}

	public int getYa() {
		return ya;
	}

	public void setYa(int ya) {
		this.ya = ya;
	}

	public int getYb() {
		return yb;
	}

	public void setYb(int yb) {
		this.yb = yb;
	}
	
}
